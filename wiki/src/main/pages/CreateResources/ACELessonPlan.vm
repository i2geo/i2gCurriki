Ext.apply(Ext.grid.GridEditor.prototype, {
	// set auto size
	autoSize: true,
	doAutoSize : function(){
		if(this.autoSize){
			var parentNode=this.boundEl.dom.parentNode;
			var width=parentNode.clientWidth;
			var height=parentNode.clientHeight;
			switch(this.autoSize){
				case "width":
					this.setSize(width,  "");
				break;
				case "height":
					this.setSize("",  height-1);
				break;
